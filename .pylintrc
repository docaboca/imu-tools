[MASTER]
init-hook='import os,sys; sys.path.insert(0,os.environ["MICROPYTHON_STUBS_PATH"])'
;init-hook='import os,sys; sys.path.insert(0,os.environ.get("MICROPYTHON_STUBS_PATH", "."))'
disable=
    global-statement,
    invalid-name,
    missing-class-docstring, missing-docstring, missing-function-docstring,
    missing-module-docstring,
    ungrouped-imports
